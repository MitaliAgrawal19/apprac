};
	
void delay(unsigned int time)
{ 
	int i,j;
	for(i=0;i<time;i++)
	for(j=0;j<10;j++);
}

void ldelay(unsigned int time)
{ 
	int i,j;
	for(i=0;i<time;i++)
	for(j=0;j<5000;j++);
}

void LCD_Strobe()
{
	IO0SET=EN;
	delay(200);
	IO0CLR=EN;
	delay(200);
}

void GLCD_cmd(unsigned char ch)
{
	IO1CLR=LCD_PORT;
	IO1SET=ch<<LCD_SHIFT;
	IO1CLR=RS;
	LCD_Strobe();
}
void GLCD_data(unsigned char ch)
{
	IO1CLR=LCD_PORT;
	IO1SET=ch<<LCD_SHIFT;
	IO1SET=RS;
	LCD_Strobe();
}


void GLCD_init()
{
	int i;
	PINSEL0=0;
	PINSEL1=0;
	PINSEL2=0;

		
	IODIR1= LCD_PORT|RS|EN|CS1|CS2|GRST|RW;
	IODIR0= LCD_PORT|RS|EN|CS1|CS2|GRST|RW;

	IOSET1=GRST|CS1|CS2;
	IOCLR1=RW|RS|EN;
	IOCLR0=RW|RS|EN;

	for(i=0;i<10;i++)
	{
		GLCD_cmd(0x3F);
		GLCD_cmd(0x40);
		GLCD_cmd(0xB8);
	}
}

void GLCD_display(unsigned char *img_arr)
{
	int page,y_addr;
	for(page=0;page<8;page++)
	 {
			IO1SET=CS1;
			IO1CLR=CS2;
			GLCD_cmd(0xB8 | page);
			GLCD_cmd(0x40);
			for(y_addr=0;y_addr<64;y_addr++)
				GLCD_data(img_arr[(page*128)+y_addr]);

			IO1SET=CS2;
			IO1CLR=CS1;
			GLCD_cmd(0xB8 | page);
			GLCD_cmd(0x40);
			for(y_addr=64;y_addr<128;y_addr++)
				GLCD_data(img_arr[(page*128)+y_addr]);
	 }
}

int main()
{
   GLCD_init();
   while(1)
   {
			GLCD_display(LED_OFF);
			delay(2000);
		 	GLCD_display(LED_ON);
			delay(2000);

   }

}








line=======================================


#include <lpc21xx.h>

#define LCD_PORT 0x00FF0000
#define EN 1<<10
#define RS 1<<25
#define CS1 1<<26
#define CS2 1<<27
#define GRST 1<<24
#define RW 1<<11
#define LCD_SHIFT 16

	
void delay(unsigned int time)
{ 
	int i,j;
	for(i=0;i<time;i++)
	for(j=0;j<10;j++);
}

void ldelay(unsigned int time)
{ 
	int i,j;
	for(i=0;i<time;i++)
	for(j=0;j<5000;j++);
}

void LCD_Strobe()
{
	IO0SET=EN;
	delay(200);
	IO0CLR=EN;
	delay(200);
}

void GLCD_cmd(unsigned char ch)
{
	IO1CLR=LCD_PORT;
	IO1SET=ch<<LCD_SHIFT;
	IO1CLR=RS;
	LCD_Strobe();
}
void GLCD_data(unsigned char ch)
{
	IO1CLR=LCD_PORT;
	IO1SET=ch<<LCD_SHIFT;
	IO1SET=RS;
	LCD_Strobe();
}


void GLCD_init()
{
	int i;
	PINSEL0=0;
	PINSEL1=0;
	PINSEL2=0;

		
	IODIR1= LCD_PORT|RS|EN|CS1|CS2|GRST|RW;
	IODIR0= LCD_PORT|RS|EN|CS1|CS2|GRST|RW;

	IOSET1=GRST|CS1|CS2;
	IOCLR1=RW|RS|EN;
	IOCLR0=RW|RS|EN;

	for(i=0;i<10;i++)
	{
		GLCD_cmd(0x3F);
		GLCD_cmd(0x40);
		GLCD_cmd(0xB8);
	}
}

void GLCD_display(unsigned char *img_arr)
{
	int page,y_addr;
	for(page=0;page<8;page++)
	 {
			IO1SET=CS1;
			IO1CLR=CS2;
			GLCD_cmd(0xB8 | page);
			GLCD_cmd(0x40);
			for(y_addr=0;y_addr<64;y_addr++)
				GLCD_data(img_arr[(page*128)+y_addr]);

			IO1SET=CS2;
			IO1CLR=CS1;
			GLCD_cmd(0xB8 | page);
			GLCD_cmd(0x40);
			for(y_addr=64;y_addr<128;y_addr++)
				GLCD_data(img_arr[(page*128)+y_addr]);
	 }
}

int main()
{
   GLCD_init();
  // while(1)
   //{
			int page,y_addr;
	IO1SET=CS2;
			IO1CLR=CS1;
	for(page=1;page<7;page++)
	 {
		 
			GLCD_cmd(0xB8 | page);
			GLCD_cmd(0x40);
			for(y_addr=0;y_addr<27;y_addr++)
				GLCD_data(0x00);
		 for(y_addr=28;y_addr<38;y_addr++)
				GLCD_data(0xFF);
		 for(y_addr=39;y_addr<64;y_addr++)
				GLCD_data(0x00);;
	 }

  // }

}
